location = /favicon.ico {
    access_log      off;
    log_not_found   off;
}

location ^~ /apple-touch-icon {
    access_log      off;
    log_not_found   off;
}

location = /robots.txt {
    access_log      off;
    log_not_found   off;
}

location = /humans.txt {
    access_log      off;
    log_not_found   off;
}

location ~ /\. {
    deny            all;
    access_log      off;
    log_not_found   off;
}
